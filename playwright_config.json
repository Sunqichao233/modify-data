{
  "websiteUrl": "https://example.com",
  "searchData": [
    "user001#Java基础课程",
    "user002#Python入门教程",
    "user003#数据结构与算法",
    "user004#Web前端开发",
    "user005#数据库设计原理",
    "user006#机器学习基础",
    "user007#React框架实战",
    "user008#Node.js后端开发",
    "user009#Vue.js项目实践",
    "user010#微服务架构设计"
  ],
  "selectors": {
    "searchInput": {
      "id": "searchInput",
      "css": "input[type='search']",
      "xpath": "//input[@placeholder='搜索' or @placeholder='Search']",
      "placeholder": "搜索",
      "name": "search"
    },
    "searchButton": {
      "id": "searchButton",
      "css": "button[type='submit']",
      "xpath": "//button[contains(text(), '搜索') or contains(text(), 'Search')]",
      "text": "搜索",
      "role": {
        "role": "button",
        "name": "搜索"
      }
    },
    "downloadButton": {
      "id": "downloadCsvBtn",
      "css": "button[class*='download']",
      "xpath": "//button[contains(text(), '下载') or contains(text(), 'Download') or contains(text(), 'CSV')]",
      "text": "下载CSV",
      "role": {
        "role": "button",
        "name": "下载"
      }
    }
  },
  "downloaderConfig": {
    "headless": false,
    "browserType": "chromium",
    "downloadDir": "./downloads",
    "timeout": 30000,
    "retryAttempts": 3,
    "humanDelay": {
      "min": 1000,
      "max": 3000
    },
    "typingDelay": {
      "min": 50,
      "max": 150
    },
    "scrollBehavior": {
      "enabled": true,
      "minScroll": 100,
      "maxScroll": 500
    },
    "antiDetection": {
      "enabled": true,
      "randomUserAgent": true,
      "disableImages": false,
      "blockAds": true
    }
  },
  "logging": {
    "enabled": true,
    "level": "info",
    "logFile": "./logs/playwright_downloader.log",
    "consoleOutput": true
  },
  "advanced": {
    "waitForNetworkIdle": true,
    "screenshotOnError": true,
    "videoRecording": false,
    "tracing": false,
    "slowMo": 0
  }
}